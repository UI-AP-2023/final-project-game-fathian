<AnchorPane prefHeight="800.0" prefWidth="1550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Login">
   <children>
      <ImageView fitHeight="802.0" fitWidth="1550.0" pickOnBounds="true">
         <image>
            <Image url="@images/clash-of-clans-skull-grenade-wallpaper-preview.jpg" />
         </image>
      </ImageView>
      <Label fx:id="username" alignment="CENTER" layoutX="89.0" layoutY="91.0" prefHeight="31.0" prefWidth="171.0" text="username: " textFill="#46250d">
         <font>
            <Font name="Edwardian Script ITC" size="49.0" />
         </font>
      </Label>
      <Label fx:id="password" alignment="CENTER" layoutX="79.0" layoutY="176.0" prefHeight="31.0" prefWidth="171.0" text="password: " textFill="#46250d">
         <font>
            <Font name="Edwardian Script ITC" size="49.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTf" layoutX="305.0" layoutY="125.0" />
      <TextField fx:id="passwordTf" layoutX="305.0" layoutY="210.0" />
